html{
	overflow-x:hidden;
}
body{
	font-family:Microsoft YaHei;
	width: 100%;
}
h1,h2,h3,h4,h5,h6{
	font-family:Microsoft YaHei;
	font-weight: 300;
    margin: 0 0 15px;
    text-transform: none;
}
._reg{
    border:0;
    margin:0;
    padding:0;
    position: fixed;
    top:0;
    width:384px;
    height:456px;
    z-index: 10001;
    display: none;
    background: #fff;
    -webkit-box-shadow: 1px 6px 36px rgba(0,0,0,.5);
    -moz-box-shadow: 1px 6px 36px rgba(0,0,0,.5);
    box-shadow: 1px 6px 36px rgba(0,0,0,.5);
    -webkit-border-radius: 5px 5px 3px 3px;
    -moz-border-radius: 5px 5px 3px 3px;
    border-radius: 5px 5px 3px 3px;
    background-color:#f1f1f1;
}
._reg iframe {
	width:100%;
	height:100%;
} 
.close-buton {
    background: url(../img/user/close.png);
    width: 25px;
    height: 25px;
    position: absolute;
    right: -12.5px;
    top: -12.5px;
    cursor: pointer;
}
